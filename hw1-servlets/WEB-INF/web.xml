<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                             http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">

  <servlet>
    <servlet-name>RequestHeaders</servlet-name>
    <servlet-class>edu.neu.hw1.RequestHeadersServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RequestHeaders</servlet-name>
    <url-pattern>/part2/headers</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>FormBasic</servlet-name>
    <servlet-class>edu.neu.hw1.FormBasicServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FormBasic</servlet-name>
    <url-pattern>/part3/basic</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>FormMap</servlet-name>
    <servlet-class>edu.neu.hw1.FormMapServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FormMap</servlet-name>
    <url-pattern>/part3/map</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>FormNames</servlet-name>
    <servlet-class>edu.neu.hw1.FormNamesServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FormNames</servlet-name>
    <url-pattern>/part3/names</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>TuitionWaiver</servlet-name>
    <servlet-class>edu.neu.hw1.TuitionWaiverServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TuitionWaiver</servlet-name>
    <url-pattern>/part4/tuition</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>RequestIntrospect</servlet-name>
    <servlet-class>edu.neu.hw1.RequestIntrospectServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RequestIntrospect</servlet-name>
    <url-pattern>/part5/introspect</url-pattern>
  </servlet-mapping>

</web-app>


